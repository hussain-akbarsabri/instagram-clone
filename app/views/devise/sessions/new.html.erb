<div class="main-content">
  <%= image_tag "instagram_image"  %>
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="field">
      <br>
      <%= f.email_field :email, autocomplete: "email", placeholder: "Email", class: "input-1" %>
    </div>

    <div class="field">
      <br>
      <%= f.password_field :password, autocomplete: "current-password", placeholder: "Password", class: "input-1" %>
    </div>

    <br>
    <div class="actions">
      <%= f.submit "Log in", class: "btn btn-primary" %>
    </div>

  <% end %>
</div>

<br>
<div class="secondary-content">
  <%= render "devise/shared/links" %>
</div>
