<div class="container">
  <div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-4">
      <br><br><br><%= image_tag("https://robogramapp.herokuapp.com/assets/auth_img1-5baaffb3d42278f4b95c51e2b134cd069383be42791c888d56514e2d2ffed552.png", cloud_name: 'instagram-dev') %>
    </div>
    <div class="col-md-7">
      <br><br><br><br><br><br><br>
      <div class="img img-thumbnail devise-form">
        <div class="main">
          <%= image_tag "https://i.imgur.com/zqpwkLQ.png"  %>
          <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
            <div class="field">
              <br><%= f.email_field :email, placeholder: "Email", required: true, class: "form-control" %>
            </div>
            <div class="field">
              <br><%= f.password_field :password, autocomplete: "current-password", required: true, placeholder: "Password", class: "form-control" %>
            </div>
            <div class="actions">
              <br><%= f.submit "Log in", class: "btn btn-primary" %>
            </div>
          <% end %>
        </div>
      </div><br><br>
      <div class="img img-thumbnail devise-form ">
        <br><%= render "devise/shared/links" %><br>
      </div>
    </div>
  </div>
</div>
