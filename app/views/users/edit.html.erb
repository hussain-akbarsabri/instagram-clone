<h1> <%= @user.email%> </h1>
<h1> <%= @user.username%> </h1>

<%= form_with(model: @user, local: true) do |form| %>
  
  <div class="field">
    <%= form.label :image %><br />
    <%= form.file_field :image, autofocus: true, autocomplete: "image" %>
  </div>
 
  <p>
    <%= form.submit %>
  </p>
 
<% end %>
